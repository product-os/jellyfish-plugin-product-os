    "lodash": "^4.17.21",
    "skhema": "^5.3.4"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^8.1.50",
    "@balena/jellyfish-types": "^1.2.39",
