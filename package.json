{
  "name": "@balena/jellyfish-plugin-product-os",
  "description": "Product OS Jellyfish Plugin",
  "version": "2.7.46",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-product-os.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "types": "node ./scripts/generate-contract-interfaces && npm run lint:fix"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.68",
    "@balena/jellyfish-logger": "^3.0.91",
    "@balena/jellyfish-plugin-base": "^2.1.251",
    "lodash": "^4.17.21",
    "skhema": "^5.3.4"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.5.4",
    "@balena/jellyfish-core": "^7.0.2",
    "@balena/jellyfish-types": "^1.2.0",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^27.0.2",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^7.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
