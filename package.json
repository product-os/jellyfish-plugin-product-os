    "@types/jest": "^27.0.3",
    "deplint": "^1.1.3",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^8.1.50"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
