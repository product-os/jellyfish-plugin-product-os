{
  "name": "@balena/jellyfish-plugin-product-os",
  "description": "Product OS Jellyfish Plugin",
  "version": "2.5.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-product-os.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run types",
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "types": "node ./scripts/generate-contract-interfaces & npm run lint-fix"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.66",
    "@balena/jellyfish-logger": "^3.0.54",
    "@balena/jellyfish-plugin-base": "^2.1.207",
    "@balena/jellyfish-types": "^0.8.12",
    "lodash": "^4.17.21",
    "skhema": "^5.3.4"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.2",
    "@balena/jellyfish-config": "^1.4.6",
    "@balena/jellyfish-core": "^5.1.11",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^26.0.24",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "simple-git-hooks": "^2.5.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^5.1.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-07-30T02:09:12.257Z"
  }
}
